{
  "name": "Widgetron",
  "description": "Widgetron shows follow dates, keeps deleted messages, and lets you build custom Twitch widgets with a built-in code editor.",
  "version": "0.5.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "client/extension/icon_16.png",
      "32": "client/extension/icon_32.png",
      "48": "client/extension/icon_48.png",
      "128": "client/extension/icon_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "icons": {
    "16": "client/extension/icon_16.png",
    "32": "client/extension/icon_32.png",
    "48": "client/extension/icon_48.png",
    "128": "client/extension/icon_128.png"
  }
}
